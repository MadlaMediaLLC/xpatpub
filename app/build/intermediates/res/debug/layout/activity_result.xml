<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_back"
    tools:context="${packageName}.${activityClass}" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#61000000" >

        <Button
            android:id="@+id/menu_button"
            android:layout_width="@dimen/width_action_button"
            android:layout_height="@dimen/height_width_action_button"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:background="@drawable/inset_menu_button"
            android:padding="@dimen/padding_activity" />

        <TextView
            android:id="@+id/myname_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="105dp"
            android:text="HI XPAT"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#ffc600"
            android:textSize="15sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/round_rect_shape1" >

        <ImageView
            android:id="@+id/my_profile_imageView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:padding="2dp"
            android:scaleType="fitXY" />
            <!-- android:src="@drawable/deal1" /> -->
    </RelativeLayout>

    <TextView
        android:id="@+id/bars_founded_count_textView"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/circle_shape1"
        android:gravity="center"
        android:text=""
        android:visibility="gone"
        android:textColor="#000000"
        android:textSize="20sp" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/relativeLayout2"
        android:layout_marginTop="30dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/tvCurrentCityHeader"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CURRENT LOCATION,"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#f5c00b"
                android:textSize="17sp"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/current_city_textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CURRENT CITY"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#ffffff"
                android:textSize="35sp" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp" >

            <LinearLayout
                android:id="@+id/choose_pub_type_button_linearLayout"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:weightSum="100" >

                <LinearLayout
                    android:id="@+id/choose_pub_type_linearLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="50"
                    android:background="@drawable/round_rect_shape2"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textView4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="See All Pubs"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#000000"
                        android:textSize="15sp" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/srch_pub_type_menu_relativeLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="50"
                    android:background="@drawable/round_rect_shape2" >

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/dropdown_mark" />

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="1dp"
                        android:layout_height="50dp"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="10dp"
                        android:layout_toLeftOf="@+id/imageView2"
                        android:background="#000000" />

                    <TextView
                        android:id="@+id/TextView02"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_toLeftOf="@+id/textView5"
                        android:gravity="center"
                        android:text="Search by Type"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textColor="#000000"
                        android:textSize="15sp" />
                </RelativeLayout>
            </LinearLayout>

            <ListView
                android:id="@+id/advertise_listView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/choose_pub_type_button_linearLayout"
                android:layout_marginTop="40dp"
                android:background="#ffc700"
                android:paddingBottom="1dp" >
            </ListView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/choose_pub_type_button_linearLayout"
                android:weightSum="100" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="50"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/privacy_setting_menubar_linearLayout"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="2dp"
                    android:layout_weight="50"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/stealth_menuitem_relativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/general_menuitem3_middle_back" >

                        <TextView
                            android:id="@+id/western_menuitem_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:text="Western"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@drawable/general_menuitem_textcolor"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/western_menuitem_radio_imageView"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/menuitem_radio_icon" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/anonymous_menuitem_relativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/general_menuitem3_middle_back" >

                        <TextView
                            android:id="@+id/sport_menuitem_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:text="Sport"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@drawable/general_menuitem_textcolor"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/sport_menuitem_radio_imageView"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/menuitem_radio_icon" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/public_menuitem_relativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/general_menuitem3_middle_back" >

                        <TextView
                            android:id="@+id/irish_menuitem_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:text="Irish"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@drawable/general_menuitem_textcolor"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/irish_menuitem_radio_imageView"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/menuitem_radio_icon" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/classic_menuitem_relativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/general_menuitem3_middle_back" >

                        <TextView
                            android:id="@+id/classic_menuitem_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:text="Neighborhood"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@drawable/general_menuitem_textcolor"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/classic_menuitem_radio_imageView"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/menuitem_radio_icon" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/taphouse_menuitem_relativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginTop="1dp"
                        android:background="@drawable/general_menuitem3_middle_back" >

                        <TextView
                            android:id="@+id/taphouse_menuitem_textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="10dp"
                            android:text="Taphouse"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="@drawable/general_menuitem_textcolor"
                            android:textSize="15sp" />

                        <ImageView
                            android:id="@+id/taphouse_menuitem_radio_imageView"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="10dp"
                            android:src="@drawable/menuitem_radio_icon" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout><!-- From: file:/C:/Users/epigraph2/workspace/xpatpub/app/src/main/res/layout/activity_result.xml -->