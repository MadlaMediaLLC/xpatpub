<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/LinearLayout2"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/general_menuitem1_top_back"
        android:orientation="vertical" >
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/less_four_relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/LinearLayout2"
        android:background="@drawable/general_menuitem1_middle_back" >

        <ImageView
            android:id="@+id/ales_sel_imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/menuitem_games_icon" />

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/ales_sel_imageView"
            android:text="Games on TV"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@drawable/general_menuitem_textcolor"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/ImageView06"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/menuitem_checkbox_icon" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/four_ten_relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/less_four_relativeLayout"
        android:background="@drawable/general_menuitem2_middle_back" >

        <ImageView
            android:id="@+id/tunes_sel_imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/menuitem_darts_icon" />

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/tunes_sel_imageView"
            android:text="Darts"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@drawable/general_menuitem_textcolor"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/ImageView04"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/menuitem_checkbox_icon" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/more_ten_relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/four_ten_relativeLayout"
        android:background="@drawable/general_menuitem1_middle_back" >

        <ImageView
            android:id="@+id/shuffleboard_sel_imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/menuitem_pool_icon" />

        <TextView
            android:id="@+id/inbox_count_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/shuffleboard_sel_imageView"
            android:text="Pool"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@drawable/general_menuitem_textcolor"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/ImageView02"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/menuitem_checkbox_icon" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/cards_relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/more_ten_relativeLayout"
        android:background="@drawable/general_menuitem2_middle_back" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/menuitem_cards_icon" />

        <TextView
            android:id="@+id/pub_info_pub_name_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/imageView1"
            android:text="Quiz Nights"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@drawable/general_menuitem_textcolor"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/menuitem_checkbox_icon" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/shuffleboard_relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/cards_relativeLayout"
        android:background="@drawable/general_menuitem1_middle_back" >

        <ImageView
            android:id="@+id/vibe_sel_imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/menuitem_shuffleboard_icon" />

        <TextView
            android:id="@+id/tunes_sel_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/vibe_sel_imageView"
            android:text="Shuffleboard"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@drawable/general_menuitem_textcolor"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/ImageView08"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/menuitem_checkbox_icon" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/LinearLayout3"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/shuffleboard_relativeLayout"
        android:background="@drawable/general_menuitem1_bottom_back"
        android:orientation="vertical" >
    </LinearLayout>

