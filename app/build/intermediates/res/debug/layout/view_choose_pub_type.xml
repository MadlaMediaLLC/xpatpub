<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="30dp" >

        <LinearLayout
            android:id="@+id/search_button_group_layout"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:weightSum="100" >

            <LinearLayout
                android:id="@+id/choose_pub_type_linearLayout"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="60"
                android:background="@drawable/round_rect_shape2"
                android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Choose Pub type"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textColor="#000000"
                    android:textSize="15sp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/search_button_group_layout"
            android:weightSum="100" >

            <LinearLayout
                android:id="@+id/privacy_setting_menubar_linearLayout"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="2dp"
                android:layout_weight="60"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/stealth_menuitem_relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/general_menuitem3_middle_back" >

                    <TextView
                        android:id="@+id/western_menuitem_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="Western"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@drawable/general_menuitem_textcolor"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/western_menuitem_radio_imageView"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/menuitem_radio_icon" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/anonymous_menuitem_relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/general_menuitem3_middle_back" >

                    <TextView
                        android:id="@+id/sport_menuitem_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="Sport"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@drawable/general_menuitem_textcolor"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/sport_menuitem_radio_imageView"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/menuitem_radio_icon" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/public_menuitem_relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/general_menuitem3_middle_back" >

                    <TextView
                        android:id="@+id/irish_menuitem_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="Irish"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@drawable/general_menuitem_textcolor"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/irish_menuitem_radio_imageView"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/menuitem_radio_icon" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/classic_menuitem_relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/general_menuitem3_middle_back" >

                    <TextView
                        android:id="@+id/classic_menuitem_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="Neighborhood"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@drawable/general_menuitem_textcolor"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/classic_menuitem_radio_imageView"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/menuitem_radio_icon" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/taphouse_menuitem_relativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/general_menuitem3_middle_back" >

                    <TextView
                        android:id="@+id/taphouse_menuitem_textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="10dp"
                        android:text="Taphouse"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@drawable/general_menuitem_textcolor"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/taphouse_menuitem_radio_imageView"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="10dp"
                        android:src="@drawable/menuitem_radio_icon" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_weight="50"
                android:orientation="vertical" >
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>
