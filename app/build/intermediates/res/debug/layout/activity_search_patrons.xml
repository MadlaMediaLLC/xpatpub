        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/RelativeLayout01"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/RelativeLayout01"
        android:text="HI JAMIE"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#ffc600"
        android:textSize="15sp" />

    <LinearLayout
        android:id="@+id/llSelectAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/user_listView"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/padding_activity_half"
